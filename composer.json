{
    "name": "romverner/laravel-advanced-relationships",
    "description": "Multi-column and JSON relationships using awobaz/compoships and staudenmeir/eloquent-json-relations",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "awobaz/compoships": "^2.0",
        "staudenmeir/eloquent-json-relations": "1.5",
        "sheldonreiff/laravel-advanced-relationships": "dev-master"
    },
    "require-dev": {
        "jeroen-g/laravel-packager": "^2.8",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [],
            "providers": [
                "Reiff\\AdvancedRelationships\\AdvancedRelationshipsServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/romverner/laravel-advanced-relationships"
        }
    ],
    "autoload": {
        "psr-4": {
            "Reiff\\AdvancedRelationships\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "@php vendor/phpunit/phpunit/phpunit"
    }
}
